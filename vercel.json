{
  "version": 2,
  "builds": [
    { "src": "vite.config.js", "use": "@vercel/static-build", "config": {} }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://employee-management-django-v61v.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
